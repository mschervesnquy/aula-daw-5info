<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Listar dados</title>
</head>
<body>
    <table>
        <tr>
            <th>ID</th>
            <th>Nome</th>
            <th>Email</th>
            <th>Foto</th>
        </tr>
        <% Usuarios.forEach(function(usuario) { %>
        <tr>
            <td>
                <%=usuario.__id%>
            </td>
            <td>
                <%=usuario.nome%>
            </td>
            <td>
                <%=usuario.email%>
            </td>
            <td>
                <%=usuario.foto%>
            </td>
        </tr>
        <% }) %>
    </table>
</body>
</html>
